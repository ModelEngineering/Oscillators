	keywords = {systems biology/oscillators},
}

@article{Novak2008,
	title = {Design principles of biochemical oscillators},
	volume = {9},
	issn = {14710072},
	doi = {10.1038/nrm2530},
	abstract = {Cellular rhythms are generated by complex interactions among genes, proteins and metabolites. They are used to control every aspect of cell physiology, from signalling, motility and development to growth, division and death. We consider specific examples of oscillatory processes and discuss four general requirements for biochemical oscillations: negative feedback, time delay, sufficient 'nonlinearity' of the reaction kinetics and proper balancing of the timescales of opposing chemical reactions. Positive feedback is one mechanism to delay the negative-feedback signal. Biological oscillators can be classified according to the topology of the positive- and negative-feedback loops in the underlying regulatory mechanism. © 2008 Macmillan Publishers Limited. All rights reserved.},
