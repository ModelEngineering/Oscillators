@Article{Papin:2004,
  Title                    = {{Comparison of network-based pathway analysis methods}},
  Author                   = {Papin, J A and Stelling, J and Price, N D and Klamt, S and Schuster, S and Palsson, B O},
  Journal                  = {Trends Biotechnol},
