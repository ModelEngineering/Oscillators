@Article{Hofmeyr2006,
  Title                    = {{Conditions for effective allosteric feedforward and feedback in metabolic pathways.}},
  Author                   = {Hofmeyr, J-H. and Rohwer, J. M. and Snoep, J. L.},
  Journal                  = {Syst Biol (Stevenage)},
