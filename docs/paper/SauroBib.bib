@Article{Savinell1992,
  Title                    = {{Network analysis of intermediary metabolism using linear optimization. I. Development of mathematical formalism.}},
  Author                   = {Savinell, J. M. and Palsson, B. O.},
  Journal                  = {J Theor Biol},
  Year                     = {1992},

