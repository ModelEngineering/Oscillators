@Article{Alberty1991,
  Title                    = {{Equilibrium compositions of solutions of biochemical species and heats of biochemical reactions.}},
  Author                   = {Alberty, R. A.},
  Journal                  = {Proc Natl Acad Sci U S A},
  Year                     = {1991},

