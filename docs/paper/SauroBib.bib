@Article{Schnell2004,
  Title                    = {Reaction kinetics in intracellular environments with macromolecular crowding: simulations and rate laws.},
  Author                   = {Schnell, S. and Turner, T. E.},
  Journal                  = {Progress in biophysics and molecular biology},
  Year                     = {2004},

