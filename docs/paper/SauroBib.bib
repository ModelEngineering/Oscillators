@Article{Karp:2007,
  Title                    = {{Multidimensional annotation of the {E}scherichia coli {K-12} genome}},
  Author                   = {Karp, P. D. and Keseler, I. M. and Shearer, A. and Latendresse, M. and Krummenacker, M. and Paley, S. M. and Paulsen, I. and Collado-Vides, J. and Gama-Castro, S. and Peralta-Gil, M. and Santos-Zavaleta, A. and Pe{\~n}aloza-Sp{\'i}nola, M. I. and Bonavides-Martinez, C. and Ingraham, J.},
