@Article{Schaff:1997,
  Title                    = {{A general computational framework for modeling cellular structure and function}},
  Author                   = {Schaff, J and Fink, C C and Slepchenko, B and Carson, J H and Loew, L M},
  Journal                  = {Biophys J},
